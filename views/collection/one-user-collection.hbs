{{!-- {{username}}
{{userId}}
{{#each images}}
{{/each}} --}}
<div class="container-fluid my-photos">
    <div class="row">

        <div class="col-md-4 card-my-photos">
            <h2>My Photos</h2>
            {{#each collections}}
            <h5>{{description}}</h5>
            <p>{{camera}}</p>
            {{#each images}}
            <article class="photo mb-5">
                <div class="card">
                    <img src="{{url}}" alt="">
                    <h5>{{title}}</h5>
                </div>
            </article>
        </div>
        {{/each}}
        {{/each}}
    </div>
</div>